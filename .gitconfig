[user]
	name = Mike Perrone
	email = mike.j.perrone@gmail.com
[alias]
    st = status
    dc = diff --cached
    d = diff
    dn = diff --name-only
    br = branch
    cm = commit -m
    co = checkout
    ap = add -p
    ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
    ri = !sh -c 'git rebase -i `git merge-base master head`'
    delete = "!del() { git branch -D $1; git push origin :$1; }; del"
[color]
    ui = true
[push]
    default = simple
[core]
	excludesfile = ~/.gitignore_global
